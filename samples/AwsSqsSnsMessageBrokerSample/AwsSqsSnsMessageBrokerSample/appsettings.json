{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MessageBroker": {
    "Provider": "AwsSqsSns",
    "AwsSqsSns": {
      "Region": "us-east-1",
      "QueueUrl": "https://sqs.us-east-1.amazonaws.com/123456789012/order-processing-queue",
      "TopicArn": "arn:aws:sns:us-east-1:123456789012:order-events",
      "UseLocalStack": true,
      "LocalStackUrl": "http://localhost:4566"
    }
  }
}
